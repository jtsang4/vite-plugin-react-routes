{
  "name": "vite-plugin-react-routes",
  "version": "0.1.0",
  "description": "A vite plugin support setup React Router by JSON config file.",
  "keywords": [
    "vite-plugin",
    "vite-plugin-react"
  ],
  "homepage": "https://github.com/jtsang4/vite-plugin-react-routes#readme",
  "bugs": {
    "url": "https://github.com/jtsang4/vite-plugin-react-routes/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jtsang4/vite-plugin-react-routes.git"
  },
  "license": "MIT",
  "author": "James Tsang <wtzeng1@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "client.d.ts"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint --ext .js,.ts .",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest"
  },
  "devDependencies": {
    "@antfu/ni": "^0.12.0",
    "@jtsang/eslint-config": "^0.20.0",
    "@types/node": "^17.0.7",
    "@types/react": "^17.0.39",
    "@types/uppercamelcase": "^3.0.0",
    "bumpp": "^7.1.1",
    "eslint": "^8.6.0",
    "esno": "^0.13.0",
    "typescript": "^4.5.4",
    "unbuild": "^0.6.7",
    "vitest": "^0.0.129"
  },
  "dependencies": {
    "esbuild": "^0.14.20",
    "jiti": "^1.12.15",
    "uppercamelcase": "^3.0.0",
    "vite": "^2.7.10"
  },
  "peerDependencies": {
    "vite": "^2.7.10"
  }
}
